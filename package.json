{
  "name": "orval-vscode",
  "displayName": "orval",
  "description": "orval is able to generate axios client with appropriate type-signatures (TypeScript) from any valid OpenAPI v3 or Swagger v2 specification, either in yaml or json formats.",
  "version": "0.2.1",
  "license": "MIT",
  "publisher": "orval",
  "repository": "https://github.com/anymaniax/orval-vscode",
  "engines": {
    "vscode": "^1.44.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.generateAll",
    "onCommand:extension.generate"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.generateAll",
        "title": "Orval: Generate all"
      },
      {
        "command": "extension.generate",
        "title": "Orval: Generate"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src/**/*.ts",
    "watch": "tsc -watch -p ./",
    "release": "dotenv release-it"
  },
  "icon": "icon.png",
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "homepage": "https://marketplace.visualstudio.com/items?itemName=orval.orval-vscode",
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@release-it/conventional-changelog": "^1.1.4",
    "@types/node": "^13.13.4",
    "@types/vscode": "^1.44.0",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "dotenv-cli": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "openapi-types": "^9.2.0",
    "prettier": "^2.0.5",
    "release-it": "^13.5.8",
    "typescript": "^4.4.2"
  },
  "dependencies": {
    "orval": "^5.5.7"
  },
  "keywords": [
    "multi-root ready",
    "javascript",
    "tsx",
    "typescript",
    "ts",
    "html",
    "vue",
    "angular",
    "react",
    "yaml",
    "yml"
  ]
}
